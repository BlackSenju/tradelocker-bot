# TradeLocker API Credentials
# Copy this file to .env and add your actual credentials
TRADELOCKER_API_KEY=your_api_key_here
TRADELOCKER_SECRET=your_secret_key_here
TRADELOCKER_ACCOUNT_ID=your_account_id_here

# Trading Bot Configuration
BOT_ENABLED=true
LIVE_TRADING=false  # Set to true only after thorough testing
DEMO_ACCOUNT=true

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=bot.log

# Data Source
DATA_SOURCE=tradelocker
TIMEFRAME=H4

# Risk Management
MAX_DAILY_TRADES=5
POSITION_SIZE=0.01
STOP_LOSS_ATR_MULT=1.5
